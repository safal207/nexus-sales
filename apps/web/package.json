                         "@types/jsonwebtoken":  "^9.0.10",
                         "bcrypt":  "^5.1.1",
                         "framer-motion":  "^12.23.22",
                         "jose":  "^6.1.0",
                         "jsonwebtoken":  "^9.0.2",
                         "next":  "^15.5.0",
                         "node-pg-migrate":  "^7.6.2",
                         "pg":  "^8.13.0",
                         "react":  "^19.1.0",
