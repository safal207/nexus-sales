{
    "name":  "web",
    "version":  "0.1.0",
    "type":  "module",
    "private":  true,
    "scripts":  {
                    "dev":  "next dev --turbopack --port 3005",
                    "build":  "prisma generate \u0026\u0026 next build",
                    "postinstall":  "prisma generate",
                    "start":  "next start",
                    "lint":  "next lint --max-warnings 0",
                    "check-types":  "tsc --noEmit",
                    "test":  "jest --config jest.config.mjs",
                    "test:watch":  "jest --config jest.config.mjs --watch",
                    "test:coverage":  "jest --config jest.config.mjs --coverage",
                    "test:ci":  "jest --config jest.config.mjs --ci --passWithNoTests",
                    "test:e2e":  "playwright test",
                    "test:e2e:ui":  "playwright test --ui",
                    "test:e2e:headed":  "playwright test --headed",
                    "test:e2e:debug":  "playwright test --debug",
                    "test:all":  "npm run test:ci \u0026\u0026 npm run test:e2e",
                    "test:integration":  "python ../../../test_ai_integration.py",
                    "migrate:create":  "node-pg-migrate create -m migrations",
                    "migrate:up":  "node-pg-migrate up -m migrations",
                    "migrate:down":  "node-pg-migrate down -m migrations",
                    "migrate:status":  "node-pg-migrate status -m migrations",
                    "verify:db":  "tsx scripts/verify-db.ts",
                    "seed":  "tsx scripts/seed.ts",
                    "seed:production":  "tsx scripts/seed-production.ts"
                },
    "dependencies":  {
                         "@dnd-kit/core":  "^6.3.1",
